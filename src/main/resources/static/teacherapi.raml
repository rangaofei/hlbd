                }
  /login:
    description: for student login
    post:
      body:
        application/json:
          type: |
            {
              "phone": String,
              "password": String
            }
          example: |
            {
              "phone": "123",
              "password": "jkli"
            }
      responses: 
        201:
          body: 
            application/json:
              type: |
                {
                  "code": int,
                  "message": String,
                  "body": {
                    "student":{
                      "studentId": String,
                      "name": String,
                      "password": String,
                      "phone": String,
                      "crteatedTime": String
                    },
                    "token": {
                      "accessToken": String,
                      "refreshToken": String
                    }
                  }
                }
              example: |
                {
                  "code": 201,
                  "message": "success",
                  "body": {
                    "student":{
                      "studentId": "15087254904257",
                      "name": "mary",
