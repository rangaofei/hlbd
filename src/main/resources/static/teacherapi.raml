/auth/student:
  description: 用于登陆注册和刷新token
  /register: # optional resource
    description: for student register
    post:
      body:
        application/json:
          type: |
            {
              "name": String,
              "phone": String,
              "password": String
            }
          example: |
            {
              "name": "mary",
              "phone": "123456",
              "password": "jkli"
            }
      responses: # declare a response
        201: # HTTP status code
          body:
            application/json:
              type: | # structural definition of a response (schema or type)
                {
                  "code": int,
                  "message": String,
                  "body":{
                    "student":{
                      "studentId": String,
                      "name": String,
                      "password": String,
                      "phone": String,
                      "crteatedTime": DatSTringe
                    },
                    "token":{
                      "accessToken": String,
                      "refreshToken": String
                    }
                  }
                }
              example: |
                {
                  "code": 201,
                  "message": "success",
                  "body": {
                    "student":{
                      "studentId": "15087254904257",
                      "name": "mary",
                      "password": "qwer",
                      "phone": "123456",
                      "crteatedTime": "1508725490425"
                    },
                    "token":{
