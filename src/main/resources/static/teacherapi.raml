/student/{student_id}:
  /joinclass:
    description: 加入班级
    post:
      queryParameters:
        class_id:
      responses:
        200:
          body:
            application/json:
              type: |
                {
                  "code": int,
                  "message": String,
                  "body": {
                    "teamId": String,
                    "teamName": String,
                    "teamIntroduction": String,
                  }
                }
              example: |
                {
                  "code": 201,
                  "message": "保存成功",
                  "body": {
                    "teamId": "6611909120",
                    "teamName": "sannianji",
                    "teamIntroduction": "haowan",
                  }
                }
      headers:
        Authorization:
          type: string
          description: access_token验证
          example: |
            Authorization: Bearer QWxhZGRpbjpvcGVuIHNlc2FtZQ==
  /getclasses:
    get:
      responses:
        200:
          body:
            application/json:
              type: |
                {
                  "code": int,
                  "message": String,
                  "body": [{
                    "teamId": String,
                    "teamName": String,
                    "teamIntroduction": String,
                  },..]
                }
              example: |
                {
                  "code": 201,
                  "message": "成功",
                  "body": [
                    {
                      "teamId": "6611909120",
                      "teamName": "sannianji",
                      "teamIntroduction": "haowan"
                    },
                    {
                      "teamId": "6611841358",
                      "teamName": "ernianji",
                      "teamIntroduction": "haowan"
                    },
                    {
                      "teamId": "6607997579",
                      "teamName": "yinianji",
                      "teamIntroduction": "haowan"
                    }
                  ]
                }
      headers:
        Authorization:
          type: string
          description: access_token验证
          example: |
            Authorization: Bearer QWxhZGRpbjpvcGVuIHNlc2FtZQ==
  /{class_id}/gethomeworks:
    get:
      responses: 
        200:
          body: 
            application/json:
              type: |
                {
                }
              example: |
                {
                }
      headers:
        Authorization:
          type: string
          description: access_token验证
          example: |
            Authorization: Bearer QWxhZGRpbjpvcGVuIHNlc2FtZQ==
    
